{
  "name": "@cardstack/boxel-motion",
  "version": "1.0.0",
  "description": "Boxel Motion Library.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "concurrently \"pnpm:build:*\" --names \"build:\"",
    "build:js": "rollup --config",
    "build:types": "glint --declaration",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:glint": "glint",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "rebuild:icons": "node bin/rebuild-icons.mjs",
    "rebuild:usage": "node bin/rebuild-usage.mjs",
    "prepack": "rollup --config",
    "start": "concurrently \"pnpm:start:*\" --names \"start:\"",
    "start:js": "rollup --config --watch --no-watch.clearScreen",
    "start:types": "glint --declaration --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "test:ember": "ember test"
  },
  "dependencies": {
    "@ember/string": "catalog:",
    "@embroider/addon-shim": "catalog:",
    "@glimmer/component": "catalog:",
    "@glint/template": "1.3.0",
    "ember-auto-import": "catalog:",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "catalog:",
    "ember-cli-typescript": "catalog:",
    "ember-concurrency": "^4.0.0",
    "ember-load-initializers": "catalog:",
    "ember-modifier": "^4.1.0",
    "file-loader": "catalog:",
    "macro-decorators": "catalog:",
    "style-value-types": "catalog:",
    "typescript": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/plugin-transform-class-properties": "catalog:",
    "@babel/plugin-transform-class-static-block": "catalog:",
    "@babel/plugin-transform-typescript": "^7.22.15",
    "@babel/runtime": "catalog:",
    "@embroider/addon-dev": "^5.0.0",
    "@embroider/macros": "^1.16.5",
    "@rollup/plugin-babel": "catalog:",
    "@tsconfig/ember": "3.0.1",
    "@glint/core": "1.3.0",
    "@glint/environment-ember-template-imports": "1.3.0",
    "@types/htmlbars-inline-precompile": "catalog:",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "catalog:",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "catalog:",
    "babel-plugin-ember-template-compilation": "^2.2.1",
    "concurrently": "^8.0.1",
    "ember-source": "catalog:",
    "ember-template-imports": "catalog:",
    "ember-template-lint": "^5.11.2",
    "ember-template-lint-plugin-prettier": "catalog:",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "catalog:",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-typescript-sort-keys": "catalog:",
    "npm-run-all": "catalog:",
    "prettier": "^3.5.1",
    "prettier-plugin-ember-template-tag": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-copy": "catalog:"
  },
  "peerDependencies": {
    "@glint/environment-ember-loose": "1.3.0",
    "ember-source": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": "20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {},
    "main": "addon-main.cjs",
    "public-assets": {},
    "type": "addon",
    "version": 2
  },
  "volta": {
    "extends": "../../../package.json"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./styles/*.css": "./dist/styles/*.css",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist",
    "public"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  }
}
