  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "node test",
      "skipFiles": ["<node_internals>/**"],
      "program": "./node_modules/.bin/../../../../node_modules/.pnpm/qunit@2.19.3/node_modules/qunit/bin/qunit.js",
      "args": [
        "--require",
        "ts-node/register/transpile-only",
        "tests/index.ts"
      ],
      "cwd": "${workspaceFolder}/packages/realm-server",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "NODE_NO_WARNINGS": "1",
        "SUPPRESS_ERRORS": "true"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start base realm",
      "skipFiles": ["<node_internals>/**"],
      "program": "./node_modules/.bin/../../../../node_modules/.pnpm/ts-node@10.9.1_wup25etrarvlqkprac7h35hj7u/node_modules/ts-node/dist/bin.js",
      "args": [
        "--transpileOnly",
        "main",
        "--port=4201",
        "--path='../base'",
        "--fromUrl='https://cardstack.com/base/'",
        "--toUrl='/base/'"
      ],
      "cwd": "${workspaceFolder}/packages/realm-server",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "NODE_NO_WARNINGS": "1"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start base and host test realms",
      "skipFiles": ["<node_internals>/**"],
      "program": "./node_modules/.bin/../../../../node_modules/.pnpm/ts-node@10.9.1_wup25etrarvlqkprac7h35hj7u/node_modules/ts-node/dist/bin.js",
      "args": [
        "--transpileOnly",
        "main",
        "--port=4201",
        "--path='../base'",
        "--fromUrl='https://cardstack.com/base/'",
        "--toUrl='/base/'",
        "--path='../host/tests/cards'",
        "--fromUrl='/test/'",
        "--toUrl='/test/'"
      ],
      "cwd": "${workspaceFolder}/packages/realm-server",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "NODE_NO_WARNINGS": "1"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start node test realm",
      "skipFiles": ["<node_internals>/**"],
      "program": "./node_modules/.bin/../../../../node_modules/.pnpm/ts-node@10.9.1_wup25etrarvlqkprac7h35hj7u/node_modules/ts-node/dist/bin.js",
      "args": [
        "--transpileOnly",
        "main",
        "--port=4202",
        "--fromUrl='/node-test/'",
        "--toUrl='/node-test/'",
        "--path='./tests/cards'",
        "--fromUrl='https://cardstack.com/base/'",
        "--toUrl='http://localhost:4201/base/'"
      ],
      "cwd": "${workspaceFolder}/packages/realm-server",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "NODE_NO_WARNINGS": "1"
      }
    }
  ]
}
