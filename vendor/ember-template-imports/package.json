{
  "name": "@cardstack/ember-template-imports",
  "version": "2.0.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-template-imports/ember-template-imports.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "npm run build",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "babel-import-util": "^0.2.0",
    "broccoli-stew": "catalog:",
    "ember-cli-babel-plugin-helpers": "catalog:",
    "ember-cli-version-checker": "catalog:",
    "line-column": "catalog:",
    "magic-string": "catalog:",
    "parse-static-imports": "catalog:",
    "string.prototype.matchall": "catalog:",
    "validate-peer-dependencies": "catalog:"
  },
  "peerDependencies": {
    "ember-cli-htmlbars": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@ember/optional-features": "catalog:",
    "@ember/test-helpers": "catalog:",
    "@embroider/test-setup": "^0.47.2",
    "@glimmer/component": "catalog:",
    "@glimmer/tracking": "^1.0.3",
    "@types/line-column": "catalog:",
    "@types/string.prototype.matchall": "catalog:",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "catalog:",
    "babel-eslint": "catalog:",
    "broccoli-asset-rev": "catalog:",
    "ember-auto-import": "catalog:",
    "ember-cli": "~3.25.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "catalog:",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "catalog:",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "catalog:",
    "ember-export-application-global": "catalog:",
    "ember-load-initializers": "catalog:",
    "ember-maybe-import-regenerator": "catalog:",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "catalog:",
    "ember-resolver": "^8.0.2",
    "ember-source": "catalog:",
    "ember-source-channel-url": "catalog:",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "catalog:",
    "eslint-plugin-node": "catalog:",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "catalog:",
    "npm-run-all": "catalog:",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "release-it": "catalog:",
    "typescript": "^5.1.6",
    "webpack": "catalog:"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-htmlbars",
      "ember-cli-babel"
    ]
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
